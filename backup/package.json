{
    "name": "game-values",
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "build": "npx vite build",
        "check": "npx run-p -ln check:*",
        "check:lint": "npx eslint .",
        "check:svelte": "npx svelte-kit sync && npx svelte-check --tsconfig tsconfig.json",
        "check:prettier": "npx prettier --check",
        "dev": "npx vite dev",
        "format": "npx run-s -ln format:*",
        "format:lint": "npx eslint . --fix",
        "format:prettier": "npx prettier --write ..",
        "preinstall": "npx only-allow bun",
        "postinstall": "npx run-s -ln prepare",
        "prepare": "npx run-p prepare:*",
        "prepare:openapi": "bun scripts/index.ts",
        "prepare:theme": "npx smui-theme compile static/styles/app.css -i src/app/assets/styles/theme"
    },
